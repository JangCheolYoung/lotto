{
  "crons": [
    {
      "path": "/api/lottery",
      "schedule": "0 21 * * 6"
    }
  ],
  "functions": {
    "app/api/lottery/route.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
